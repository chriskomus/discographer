<%= render @video %>

<div>
  <%= link_to "Edit this video", edit_video_path(@video), class: "btn btn-md btn-danger m-2" %> |
  <%= link_to "Back to videos", videos_path, class: "btn btn-md btn-danger m-2" %>
  <%= button_to "Destroy this video", @video, method: :delete, class: "btn btn-md btn-danger m-2" %>
</div>
